{
  "name": "police-officer-employment-history",
  "version": "1.0.0",
  "description": "c21 capstone project",
  "main": "index.js",
  "author": "yy",
  "license": "MIT",


  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.11",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "dotenv-flow": "^4.1.0",
    "express": "^4.19.2",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "typeorm": "0.3.20"
  },
  "scripts": {
    "build": "tsc",
    "start": "cross-env NODE_ENV=production node build/index.js",
    "dev:start": "cross-env NODE_ENV=development  ts-node-dev --respawn --transpile-only ./src/index.ts",
    "test": "cross-env  jest --config jest.config.js --coverage --detectOpenHandles",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "[__for TypeORM migration": "",
    "migration:create": "ts-node ./node_modules/typeorm/cli.js migration:create",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run -- -d src/data-source.ts",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert -- -d src/data-source.ts",
    "schema:drop": "ts-node ./node_modules/typeorm/cli.js schema:drop",
    "schema:sync": "ts-node ./node_modules/typeorm/cli.js schema:sync -d src/data-source.ts",
    "migration:show": "ts-node ./node_modules/typeorm/cli.js migration:show -- -d src/data-source.ts",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -d src/data-source.ts",
    "]__for TypeORM migration": ""
  }
}
